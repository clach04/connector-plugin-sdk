    <function group='cast' name='FLOAT' return-type='real'>
      <formula>CAST(CASE WHEN %1 IS TRUE THEN 1.0 WHEN %1 IS FALSE THEN 0.0 ELSE NULL END AS DOUBLE PRECISION)</formula>
      <argument type='bool' />
    </function>
    <function group='cast' name='INT' return-type='int'>
	
