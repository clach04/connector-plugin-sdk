      <formula>(CASE WHEN %2 IS NULL OR %3 IS NULL THEN NULL&#10;      WHEN CAST(%3 AS INTEGER) &lt; 1 THEN &apos;&apos;&#10;      WHEN CAST(%2 AS INTEGER) &lt; 1 THEN SUBSTR(%1,1,CAST(%3 AS INTEGER))&#10;      ELSE SUBSTR(%1, CAST(%2 AS INTEGER), CAST(%3 AS INTEGER)) END)</formula>
