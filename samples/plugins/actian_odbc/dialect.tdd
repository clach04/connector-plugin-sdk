	  <formula>(CASE WHEN POSITION(%1 in %2) &gt; 0 AND POSITION(%1 in %2)&lt; LENGTH (%2) THEN TRUE ELSE FALSE END)</formula>
      <argument type='str' />
      <argument type='str' />
    </function>
